    "express-session": "^1.15.6",
    "heroku": "^7.21.0",
    "jquery": "^3.3.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.14.7",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
